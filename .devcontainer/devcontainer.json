{
  "name": "Node.js Sample",
  "dockerComposeFile": "docker-compose.yml",
  "service": "web",
  "workspaceFolder": "/workspace",
  "appPort": [3000],
  "remoteUser": "node",
  "settings": {
    "terminal.integrated.shell.linux": "/usr/bin/zsh"
  },
  "postCreateCommand": "yarn install",
  "extensions": ["esbenp.prettier-vscode", "gamunu.vscode-yarn"]
}
